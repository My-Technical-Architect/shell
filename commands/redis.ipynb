    "# install\n",
    "## mac install\n",
    "\n",
    "```sh\n",
    "brew install redis\n",
    "# 配置文件\n",
    "/usr/local/etc/redis.conf\n",
    "\n",
    "# 可在redis.conf 中的requirepass 设置密码\n",
    "\n",
    "# 启动服务\n",
    "sudo ./redis-server\n",
    "\n",
    "# 客户端登录\n",
    "redis-cli -h 127.0.0.1 -p 6379 -a XXX\n",
    "\n",
    "```\n"
   ]
  }
